@model HospitalSchedule.Models.Exchange_Request

@{
    ViewData["Title"] = "Confirm Exchange Request";
}

<h2>Confirm Exchange Request</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <p>Are you sure you want to request this schedule exchange?</p>
            </div>
            <div class="form-group">
                <input type="submit" value="Confirm" class="btn btn-default" />
                <a asp-action="Index" class="btn btn-primary">Cancel</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
