<body id="body">
   


    <!-- #header -->
    <header id="header">
        <div class="container">
            <div id="logo" class="pull-left">
                <h1><a>Hospital</a></h1>         
            </div>
            <nav id="nav-menu-container">
                <ul class="nav-menu">
                    <li class="menu-active"><a href="#body">Home</a></li>
                    <li><a href="#team">Team</a></li>
                    <li><a href="#contact">Contact</a></li>
                </ul>
            </nav><!-- #nav-menu-container -->
        </div>
    </header>





    <!--==========================
      Intro Section
    ============================-->
    <section id="intro">
        <div class="intro-content">
            <h2>Welcome <span>to ULS </span><br>Guarda</h2>
            <div>
                <a href="#Creation" class="btn-get-started scrollto">Creation Page</a>
                <a href="#Timetables" class="btn-projects scrollto">Timetables</a>
            </div>
        </div>
        <div id="intro-carousel" class="owl-carousel">
            <div class="item" style="background-image: url('img/intro-carousel/1.jpg');"></div>
        </div>
    </section>

    <main id="main">


        <!--==========================
          Our Team Section
        ============================-->
        <section id="team" class="wow fadeInUp">
            <div class="container">
                <div class="section-header">
                    <h2>Our Team</h2>
                </div>
                <div class="row">
                    <div class="col-lg-2 col-md-6">
                        <div class="member">
                            <div class="pic"><img src="img/team-1.jpg" alt=""></div>
                            <div class="details">
                                <h4>Luciano Brido</h4>
                                <span>Student</span>
                                <span>1011881</span>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6">
                        <div class="member">
                            <div class="pic"><img src="img/team-2.jpg" alt=""></div>
                            <div class="details">
                                <h4>Leonardo Lourenço</h4>
                                <span>Student</span>
                                <span>1011881</span>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6">
                        <div class="member">
                            <div class="pic"><img src="img/team-3.jpg" alt=""></div>
                            <div class="details">
                                <h4>Bruno Santos</h4>
                                <span>Student</span>
                                <span>1011881</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>


        <!--==========================
          Contact Section
        ============================-->
        <section id="contact" class="wow fadeInUp">
            <div class="container">
                @RenderBody()
                <div class="section-header">
                    <h2>Contact Us</h2>
                </div>
                <div class="row contact-info">
                    <div class="col-md-4">
                        <h3>Instituto Politécnico da Guarda</h3>
                        <div class="contact-address">
                            <i class="ion-ios-location-outline"></i>
                            <h3>Address</h3>
                            <address>
                                Avenida Dr. Francisco Sá Carneiro, n.º 50
                                6300-559 Guarda
                            </address>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="contact-phone">
                            <i class="ion-ios-telephone-outline"></i>
                            <h3>Phone Number</h3>
                            <p><a href="tel: (+351) 271 220 100"> (+351) 271 220 100</a></p>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="contact-email">
                            <i class="ion-ios-email-outline"></i>
                            <h3>Email</h3>
                            <p><a href="ipg@ipg.pt ">ipg@ipg.pt </a></p>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="wow shake" data-wow-delay="0.4s">
                            <div class="page-scroll marginbot-30">
                                <a href="#intro" id="totop" class="btn btn-circle">
                                    <i class="fa fa-angle-double-up animated"></i>
                                </a>
                            </div>
                            <p>&copy;&nbsp;Grupo 11</p>
                            <div class="credits">
                                <p>Engenharia Informática - IPG</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section><!-- #clients -->
